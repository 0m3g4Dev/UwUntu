{
  "name": "uwuntu",
  "description": "Ubuntu emulator",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run minify-html",
    "minify-html": "html-minifier-terser --input-dir dist --output-dir dist --file-ext html --collapse-whitespace",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "keywords": [
    "vanilla",
    "ubuntu",
    "terminal"
  ],
  "license": "MIT",
  "author": "Eber contact@doneber.dev",
  "homepage": "https://uwuntu.doneber.dev",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "html-minifier-terser": "7.2.0",
    "standard": "17.1.0",
    "typescript": "5.2.2",
    "vite": "4.4.8"
  }
}
